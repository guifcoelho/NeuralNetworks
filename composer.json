{
  "name": "guifcoelho/NeuralNetworks",
  "description": "Package for training and predicting with neural networks",
  "license": "MIT",
  "keywords": [
    "Neural networks"
  ],
  "authors": [
    {
      "name": "Guilherme Coelho",
      "email": "coelhoxz@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "illuminate/config": "5.2.*"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*",
    "mockery/mockery": "0.*",
    "symfony/var-dumper": "2.*"
  },
  "autoload": {
    "psr-4": {
      "guifcoelho\\NeuralNetworks\\": "src",
      "NeuralNetworks\\Library\\": "src/library"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "guifcoelho\\NeuralNetworks\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "minimum-stability": "stable"
}
